{
    "log": {
      "level": "fatal",
      "timestamp": true
    },
    "dns": {
      "servers": [
        {
          "tag": "dns-remote",
          "address": "tls://1.1.1.1",
          "address_strategy": "ipv4_only",
          "detour": "proxy"
        },
        {
          "tag": "dns-local",
          "address": "local"
        }
      ],
      "rules": [
        {
          "rule_set": [
            "refilter-domains"
          ],
          "server": "dns-remote"
        },
        {
          "rule_set": [
            "refilter-ipsum"
          ],
          "server": "dns-remote"
        }
      ],
      "final": "dns-local"
    },
    "inbounds": [
      {
        "type": "tun",
        "tag": "tun-in",
        "interface_name": "freedom",
        "stack": "system",
        "address": "172.19.0.1/28",
        "auto_route": true,
        "strict_route": true,
        "sniff": true,
        "sniff_override_destination": true
      }
    ],
    "outbounds": [
      {
        "type": "direct",
        "tag": "direct"
      },
      {
        "type": "selector",
        "tag": "proxy",
        "outbounds": null
      },
      {
        "type": "urltest",
        "tag": "Fastest",
        "outbounds": null,
        "url": "https://www.gstatic.com/generate_204",
        "interval": "15m0s"
      }
    ],
    "route": {
      "rules": [
        {
          "action": "sniff"
        },
        {
          "protocol": "dns",
          "action": "hijack-dns"
        },
        {
          "rule_set": [
            "refilter-domains"
          ],
          "outbound": "proxy"
        },
        {
          "rule_set": [
            "refilter-ipsum"
          ],
          "outbound": "proxy"
        }
      ],
      "rule_set": [
        {
          "tag": "refilter-domains",
          "type": "remote",
          "format": "binary",
          "url": "https://github.com/1andrevich/Re-filter-lists/releases/latest/download/ruleset-domain-refilter_domains.srs"
        },
        {
          "tag": "refilter-ipsum",
          "type": "remote",
          "format": "binary",
          "url": "https://github.com/1andrevich/Re-filter-lists/releases/latest/download/ruleset-ip-refilter_ipsum.srs"
        }
      ],
      "auto_detect_interface": true,
      "override_android_vpn": true
    },
    "experimental": {
      "cache_file": {
        "enabled": true
      }
    }
  }